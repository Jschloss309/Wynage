<div class="form_container">
<div class="login_column">
  <h1 id="log_in">Log In</h1>

  <%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :email %><br>
    <%= text_field_tag :email %>
  </div>

  <div class="field">
    <%= label_tag :password %><br>
    <%= password_field_tag :password %>
  </div>

  <div class="actions">
    <%= submit_tag "log In" %>
  <% end %>
</div>
</div>
